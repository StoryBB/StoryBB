{% extends '@layouts/admin.twig' %}

{% block title %}{{ phrase('General:login') }}{% endblock %}

{% block content %}
	<x-admin.panel>
		<x-admin.panel.heading>{{ phrase('General:login') }}</x-admin.panel.heading>
		<x-admin.panel.body>
			{% if login_errors %}
				<x-admin.error-list errors=login_errors />
			{% endif %}
			{{ form|raw }}
		</x-admin.panel.body>
	</x-admin.panel>
{% endblock %}