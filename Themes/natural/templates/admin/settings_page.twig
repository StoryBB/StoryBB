{% extends '@layouts/admin.twig' %}
{% import '@partials/admin/panels.twig' as panel %}

{% block title %}{{ title }}{% endblock %}

{% block content %}
	{{ panel.begin() }}
		{{ panel.heading( title ) }}
		{{ panel.body_begin() }}
			{{ form|raw }}
		{{ panel.body_end() }}
	{{ panel.end() }}
{% endblock %}